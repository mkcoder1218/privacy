{
  "version": 2,
  "builds": [
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "*.css", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/privacy", "dest": "/privacy-policy.html" },
    { "src": "/privacy-policy", "dest": "/privacy-policy.html" }
  ]
}